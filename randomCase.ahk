Insert::
    Send, {ScrollLock}
    LockToolTip("ScrollLock")
Return

~a::
    If (GetKeyState("ScrollLock", "T") == 1){
        Random, Var,0,2
        If (Var != 0){
            Send, {BackSpace}A
        } Else {
            Send, {BackSpace}a
        }
    }
return

